    "webpack-manifest-plugin": "1.2.1",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build:server": "NODE_ENV='production' webpack --config config/webpack.config.server.prod.js --progress",
    "build:client": "NODE_ENV='production' webpack --config config/webpack.config.client.prod.js --progress",
    "build": "yarn run build:server && yarn run build:client",
    "start:server": "NODE_ENV='production' node build/server.js",
    "start:devserver": "NODE_ENV='development' STYLEDASH_WEBPACK_CONFIG='config/webpack.config.client.dev.js' babel-watch src/server.js",
    "start:lint": "esw src/ -w",
    "check:flow": "flow",
    "check:lint": "esw src/",
    "check": "yarn run check:flow && yarn run check:lint",
    "setup:hooks": "./scripts/linkHooks.sh",
    "deploy:pre": "yarn run build",
    "test": "node scripts/test.js --env=jsdom",
    "parse-debug": "babel-node src/scripts/parseRepo.js"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-watch": "^2.0.7",
    "relay-compiler": "^1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
